Algoritmo "exercicio13"

Var

  a, b, c, d, k, z: real

Inicio

  escreva ("Informe o valor da hora aula: ")
  leia (a)
  escreva ("Informe o número de aulas dadas ao mês: ")
  leia (b)
  escreva ("Informe o percentual de desconto do INSS: ")
  leia (c)
  escreval
  d <- (a * b)
  k <- (c/100) * d
  z <- d - k


  escreva ("Salario líquido: ", z)

Fimalgoritmo