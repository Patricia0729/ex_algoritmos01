Algoritmo "exercicio15"

Var

  t, v, d, l: real

Inicio

   escreva ("Informe o tempo gasto na viagem: ")
   leia (t)
   escreva ("Informe a velocidade média: ")
   leia (v)
   d <- t * v
   l <- d/12
   escreval
   escreval ("LITROS UTILIZADOS: ", l:2:1)
   escreval ("DISTÂNCIA PERCORRIDA: ", d)

Fimalgoritmo