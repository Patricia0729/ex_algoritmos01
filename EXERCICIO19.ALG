Algoritmo "exercicio19"

Var

   a, b, x: real

Inicio

   escreva ("Digite o primeiro valor de A: ")
   leia (a)
   escreva ("Digite o primeiro valor de B: ")
   leia (b)
   escreval
   x <- a
   a <- b
   b <- x
   escreval ("O novo valor de A será: ", a)
   escreval ("O novo valor de B será: ", b)

Fimalgoritmo